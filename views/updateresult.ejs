<%-include('common/header');-%>
<%-include('common/loginnav');-%>
    <button type="button" style="margin-left:100px;" onclick="location.href='display';" class="btn btn-success">All Result</button>

    <div class="form-title text-center">
        <h2 class="text-dark">
            Update result
        </h2>
    </div>
<div class="container">
    <form method="GET" id="updateresult">
        <div class="new_user">
            <div class="form-group">
                <label for="name" class="text-light">Name</label>
                <input type="hidden" name="RollNo" value="<%= studentResult.RollNo %>">
                <input type="text" name="Name" value="<%=studentResult.Name%>">
            </div>
            <div class="form-group">
                <label for="name" class="text-light">Date of Birth</label>
                <input type="date" name="Date" value="<%=studentResult.DOB%>">
            </div>
            <div class="form-group">
                <label for="name" class="text-light">Score</label>
                <input type="number" name="Score" value="<%=studentResult.Score%>">
            </div>
            <div class="form-group">
                <button type="submit" class="btn btn-primary">Save</button>
            </div>
        </div>

    </form>
  </div>
<%-include('common/footer');-%>